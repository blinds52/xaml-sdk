<Window x:Class="AsyncData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:local="clr-namespace:AsyncData"
        Title="MainWindow" Height="350" Width="625">
    <Grid>
        <TabControl>
            <TabItem Header="Basic Chart">
                <local:BasicChart DataContext="{Binding BasicChartVM}" />
            </TabItem>
            <TabItem Header="Dispatched Collection">
                <local:DispatchedCollection DataContext="{Binding AsyncVM}" />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
